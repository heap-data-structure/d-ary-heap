{
  "name": "@heap-data-structure/d-ary-heap",
  "description": "d-ary heap data structure library for JavaScript",
  "version": "12.0.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://heap-data-structure.github.io/d-ary-heap",
  "repository": {
    "url": "https://github.com/heap-data-structure/d-ary-heap",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/heap-data-structure/d-ary-heap/issues"
  },
  "keywords": [
    "binary",
    "computer science",
    "d-ary",
    "data",
    "data structures",
    "heap",
    "javascript",
    "js",
    "library",
    "structure"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --env-name production src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "dev": "npm run lint -- --fix && npm run cover -- -- -st --fail-fast",
    "install-hooks": "husky install",
    "lint": "true",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava",
    "travis": "npm run lint && npm run cover"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-array": "4.0.0",
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/register": "7.14.5",
    "@comparison-sorting/is-sorted": "^0.0.1",
    "@functional-abstraction/functools": "^3.0.2",
    "@heap-data-structure/specification": "^16.0.0",
    "@iterable-iterator/chain": "^2.0.1",
    "@iterable-iterator/consume": "^1.0.1",
    "@iterable-iterator/list": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@iterable-iterator/range": "^2.0.1",
    "@randomized/random": "^4.0.0",
    "@set-theory/cartesian-product": "^2.0.1",
    "@total-order/primitive": "^1.0.1",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.1.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.8.0",
    "coveralls": "3.1.1",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "7.0.1",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.9",
    "xo": "0.42.0"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m",
    "concurrency": 8
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "babel-preset-power-assert"
        ],
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      },
      "production": {
        "plugins": [
          "babel-plugin-unassert",
          [
            "transform-remove-console",
            {
              "exclude": [
                "log",
                "error",
                "warn"
              ]
            }
          ]
        ],
        "sourceMaps": "inline"
      }
    }
  }
}
